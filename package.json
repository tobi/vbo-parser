{
  "name": "@vbo-parser/core",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript library for parsing and analyzing VBO (Vehicle Bus Observer) telemetry files from motorsport data acquisition systems",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build && bun run test"
  },
  "keywords": [
    "vbo",
    "motorsport",
    "telemetry",
    "data-acquisition",
    "racing",
    "lap-analysis",
    "gps",
    "automotive",
    "parser",
    "typescript"
  ],
  "author": {
    "name": "VBO Parser Team",
    "url": "https://github.com/vbo-parser/core"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vbo-parser/core.git"
  },
  "bugs": {
    "url": "https://github.com/vbo-parser/core/issues"
  },
  "homepage": "https://github.com/vbo-parser/core#readme",
  "dependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}